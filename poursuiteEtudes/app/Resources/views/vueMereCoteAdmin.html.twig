<!DOCTYPE html>

<html>
<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<!-- Latest compiled and minified JavaScript -->
<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.js"></script>

<link rel="stylesheet" type="text/css" href="/styles/style2.css">
    <head>

        <meta charset="UTF-8" />

        {% block title %}<title> Poursuite d'etudes </title> {% endblock %}

        {% block stylesheets %}
        
        
        
        {% endblock %}
    
    </head>

    <body>

        {% block header %}
          
        <header >
            
              <div class="container">
          <!-- Ligne 1 --> 
                  <div class="row">
                      <div class="col-lg-2">
                    <!-- contenu premier bloc 2 colonnes -->  
                          <img alt="Accueil" src="/images/iutlogo.png" >
                      </div>
                    
                      <div class="col-lg-12">
                      <h1> <center>Application Web Poursuite d'études</center></h1> 
                         <h2>
                          <br /> 
                          <center>Le site référence de la poursuite d'études au département informatique de Bayonne</center>
                          <br /> 
                         </h2>
                      </div>
                  </div>    
              </div>
           
        </header>
          

        {% endblock %}

        {% block menuGeneral %}
     
              <div class="container">
                <div class="row">
                  <div class="col-lg-3">
                    
<nav class="navbar navbar-inverse">
  <div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="{{ path('poursuite_etudes_formations_admin')}}">
        <span class="glyphicon glyphicon-home" aria-hidden="true"></span>
      </a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
       <ul class="nav navbar-nav">
        <!-- <li><a href="#">  Accueil <span class="sr-only"></span></a></li> -->
        <li><a href="{{ path('poursuite_etudes_formations_etudiantAdmin') }}"> Etudiants </a></li>
        <li><a href="{{ path('poursuite_etudes_formations_ecoleIngeAdmin') }}"> Ecoles d'ingenieur </a></li>
        <li><a href="{{ path('poursuite_etudes_formations_licencesAdmin') }}"> Licences </a></li>
        <li><a href="{{ path('poursuite_etudes_formations_licencesProAdmin') }}"> Licences professionnelles </a></li>
        <li><a href="{{ path('poursuite_etudes_formations_statsAdmin') }}"> Statistiques </a></li>
        <li><a href="{{ path('poursuite_etudes_formations_informationsAdmin') }}"> Informations </a></li>
        <li><a href="{{ path('poursuite_etudes_formations_nouvelleAnneeAdmin') }}"> Nouvelle année </a></li>
        <li role="separator" class="divider"></li>
        <li><center><button type="button" class="btn btn-primary">Revenir au site</button></center> </li>
       </ul>
       
       
    </div> <!-- collapse -->
  </div> <!-- container fluid -->
                   
                </div> <!-- col lg 3 -->
                <div class="col-ls-9"> <!-- deuxieme bloc de la deuxieme ligne -->
                  
                   {% block contenuCentral %}  {% endblock %}   
                        
                </div>
                </div> <!-- row -->
              </div> <!-- container-->
  
        {% endblock %}

        {% block menuPropreAuBundle %} {% endblock %}
        
        
            <div class="container">
              <div class="row">
                <div class="col-lg-12">
                  
                  {% block footer %} 
                  <HR>
                  <footer> <center> &copy; IUT de Bayonne - Departement informatique </center> </footer>
                  <HR>
                  {% endblock %}
                  
                </div>
              </div>
            </div>
        
          
        
        

    </body>

</html>